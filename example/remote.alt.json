{
    "directory": "../",
    "remotehost": "walmartv5.vtexcommercebeta.com.br",
    "remoteport": 80,
    "localhost": "localhost",
    "localport": 3000,
    "headers": {
        "Host": "walmartv5.vtexcommercebeta.com.br",
        "X-Track" : "mkp1"
    },
    "bounceToRemote": true,
    "bounces":[
        ".*/api/.*",
        ".*/api/checkout/pub/.*",
        ".*/api/checkout/pub/.*/.*",
        ".*/api/checkout/pub/.*/.*/.*",
        ".*/transaction",
        ".*/checkout/gatewayCallback.*",
        ".*/checkout/cart/add.*"
    ],
    "mapping": false,
    "mappings": {
        ".*/api/checkout/pub/orderForm/.*":"./test/mocks/orderFormResponse-cep.json",
        ".*/checkout/js/checkout-vm.js":"./src/js/orderform/checkout-vm.js"
    }
}
